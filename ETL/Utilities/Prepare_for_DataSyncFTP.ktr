  <step>
    <name>Characters to Strip</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>N</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#x2f;&#x2f;Script here&#xa;&#xa;var OSType&#xa;var CharactersToStrip  &#x2f;&#x2f;8 for Windows, 7 for Linux&#xa;&#xa;var WinPosition &#x3d; OS.indexOf&#x28;&#x22;Windows&#x22;&#x29;&#xa;var LinuxPosition &#x3d; OS.indexOf&#x28;&#x22;Linux&#x22;&#x29;&#xa;var MacPosition &#x3d; OS.indexOf&#x28;&#x22;Mac&#x22;&#x29;&#xa;&#xa;if &#x28;WinPosition &#x3e;&#x3d; 0&#x29;&#xa;	&#x7b;&#xa;	OSType &#x3d; &#x22;Windows&#x22;&#x3b;&#xa;	CharactersToStrip &#x3d; 8&#x3b;&#xa;	&#x7d;&#xa;else if &#x28;LinuxPosition &#x3e;&#x3d;0&#x29;&#xa;	&#x7b;&#xa;	OSType &#x3d; &#x22;Linux&#x22;&#x3b;&#xa;	CharactersToStrip &#x3d; 7&#x3b;&#xa;	&#x7d;&#xa;else if &#x28;MacPosition &#x3e;&#x3d;0&#x29;&#xa;	&#x7b;&#xa;	OSType &#x3d; &#x22;Mac&#x22;&#x3b;&#xa;	CharactersToStrip &#x3d; 7&#x3b;&#xa;	&#x7d;&#xa;else&#xa;	&#x7b;&#xa;	OSType &#x3d; &#x22;Unknown - Guessing at Characters to Strip. Job may fail.&#x22;&#x3b;&#xa;	CharactersToStrip &#x3d; 7&#x3b;&#xa;	&#x7d;&#xa;</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>OSType</name>
        <rename>OSType</rename>
        <type>String</type>
        <length>-1</length>
        <precision>-1</precision>
        <replace>N</replace>
      </field>      <field>        <name>CharactersToStrip</name>
        <rename>CharactersToStrip</rename>
        <type>Number</type>
        <length>2</length>
        <precision>-1</precision>
        <replace>N</replace>
      </field>    </fields>     <cluster_schema/>
